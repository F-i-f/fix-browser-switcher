{
  "env": {
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "globals": {
    "console": "readonly",
    "imports": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error",
    "semi": ["warn", "always"],
    "quotes": ["warn", "single"],
    "indent": ["warn", 4],
    "no-trailing-spaces": "warn",
    "eol-last": "warn",
    "no-multiple-empty-lines": ["warn", { "max": 1 }]
  }
}
